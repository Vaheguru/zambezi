#!groovy
// Check ub1 properties
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '30', artifactNumToKeepStr: '30'))
        timestamps()
    }
    stages {
        stage("create docker image") {
            steps {
               echo "------------ start building image---------------" 
               dir {'docker/toolbox'} {
               sh 'docker build . '
            }
        }
    }
  }
}

